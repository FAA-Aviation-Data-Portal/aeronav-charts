{
  "name": "@faa-aviation-data-portal/aeronav-charts",
  "description": "Fetch links and metadata for charts provided by Aeronav in a sane format.",
  "version": "1.0.0",
  "author": {
    "name": "Forrest Desjardins",
    "email": "desjardinsfg@gmail.com",
    "url": "github.com/fdesjardins"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "superagent": "^8.0.9"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "coveralls": "^3.1.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "standard": "^17.1.0"
  },
  "engines": {
    "node": ">=14"
  },
  "eslintConfig": {
    "extends": [
      "standard"
    ],
    "rules": {
      "object-curly-spacing": [
        2,
        "always"
      ],
      "array-bracket-spacing": [
        2,
        "always"
      ],
      "func-style": [
        2,
        "expression"
      ],
      "prefer-arrow-callback": [
        2,
        {
          "allowNamedFunctions": false
        }
      ]
    }
  },
  "files": [
    "index.js",
    "lib/",
    "license",
    "readme.md"
  ],
  "keywords": [
    "aeronav",
    "charts",
    "digital products",
    "ifr",
    "plannning",
    "sectionals",
    "terminal area",
    "vfr"
  ],
  "license": "MIT",
  "main": "index.js",
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "private": false,
  "repository": "FAA-Aviation-Data-Portal/aeronav-charts",
  "scripts": {
    "test": "standard *.js && nyc mocha test.js",
    "standard:fix": "standard --fix"
  }
}
